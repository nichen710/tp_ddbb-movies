# Root level gitignore for full-stack movie application

# Environment variables
.env
.env.staging

# backend (Python/FastAPI)
backend/__pycache__/
backend/*.pyc
backend/*.pyo
backend/*.pyd
backend/.Python
backend/build/
backend/develop-eggs/
backend/dist/
backend/downloads/
backend/eggs/
backend/.eggs/
backend/lib/
backend/lib64/
backend/parts/
backend/sdist/
backend/var/
backend/wheels/
backend/*.egg-info/
backend/.installed.cfg
backend/*.egg
backend/logs/
backend/.pytest_cache/
backend/.coverage
backend/htmlcov/

# frontend (Node.js/Next.js)
frontend/node_modules/
frontend/.next/
frontend/out/
frontend/build/
frontend/dist/
frontend/*.tsbuildinfo
frontend/.vercel
frontend/.turbo

# IDE and Editors
.vscode/
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Docker
.dockerignore

# Logs
*.log
logs/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

